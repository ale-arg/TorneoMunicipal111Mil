<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="modelo.Escuela" table="escuela" catalog="`TORNEO-MUNICIPAL-111MIL`">
        <id name="idEscuela" type="java.lang.Integer">
            <column name="idEscuela" />
            <generator class="identity"></generator>
        </id>
        <property name="nombreEscuela" type="string">
            <column name="nombreEscuela" length="50" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
